    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "sonarscan": "node ./sonarscan.js"
  },
  "dependencies": {
    "@ant-design/plots": "^2.3.3",
    "@sentry/react": "^9.15.0",
    "@sentry/vite-plugin": "^3.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.5.2",
    "redux-persist": "^6.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@reduxjs/toolkit": "^2.2.6",
    "@sonar/scan": "^4.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/redux-persist": "^4.0.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "antd": "^5.19.3",
    "eslint": "^8.57.1",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "js-cookie": "^3.0.5",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
